// Digits in a number
#include <iostream>
using namespace std;

string isPrime(int n){
    for(int i=2; i*i <= n; i++){
        if(n % i == 0){
            return "Non prime";
        }
    }
    return "Prime";
}

void printDigits(int n){
    while(n != 0){
        int digit = n % 10;
        cout << digit << endl;
        
        n = n/10;
    }
}

int main() {
    int n = 3568;
    
    printDigits(n);


    return 0;
}
